<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.uiyeestudio.scan">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.uiyeestudio.scan.ScanApplication"
        android:allowBackup="true"
        android:icon="@mipmap/launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.uiyeestudio.scan.HomeActivity">
        </activity>
        <activity android:name="com.uiyeestudio.scan.MainScanActivity" />
        <activity
            android:name="com.uiyeestudio.scan.ContinuousScanActivity"
            android:label="@string/title_activity_continuous"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.uiyeestudio.scan.ListActivity" />
        <activity android:name="com.uiyeestudio.scan.HandlerTestActivity" />
        <activity
            android:name="com.uiyeestudio.scan.CustomScannerActivity"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.uiyeestudio.scan.CommodityDetailActivity" />
        <activity android:name="com.uiyeestudio.scan.HistoryActivity"></activity>
    </application>

</manifest>